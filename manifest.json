{
  "manifest_version": 2,

  "name": "Url-Logger",
  "description": "This extension tracks the history of user-visited URLs",
  "version": "1.1",

  "background": {
    "scripts": ["mostvisited.js","insertdb.js"]
  },
  
  "browser_action": {
    "default_icon": "URL.jpg",
    "default_popup": "popup.html",
	"default_title": "URL Logger"
  },
  
  "permissions": [
    "activeTab",
	"background",
	"history",
	"https://ajax.googleapis.com/",
	"http://Sample-env.zssmubuwik.us-west-1.elasticbeanstalk.com/*"
  ]
}
